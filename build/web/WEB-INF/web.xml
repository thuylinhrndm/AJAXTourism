<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.0" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd">
    <servlet>
        <servlet-name>TourismHome</servlet-name>
        <servlet-class>TourismHome</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>VietNam</servlet-name>
        <servlet-class>VietNam</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Oman</servlet-name>
        <servlet-class>Oman</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Country</servlet-name>
        <servlet-class>Country</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>VietNamExist</servlet-name>
        <servlet-class>VietNamExist</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Festival</servlet-name>
        <servlet-class>Festival</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Festivals</servlet-name>
        <servlet-class>Festivals</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>TourismHome</servlet-name>
        <url-pattern>/TourismHome</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>VietNam</servlet-name>
        <url-pattern>/VietNam</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Oman</servlet-name>
        <url-pattern>/Oman</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Country</servlet-name>
        <url-pattern>/Country</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>VietNamExist</servlet-name>
        <url-pattern>/VietNamExist</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Festival</servlet-name>
        <url-pattern>/Festival</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Festivals</servlet-name>
        <url-pattern>/Festivals</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
